        "@mui/material": "5.15.19",
        "@mui/x-charts": "7.8.0",
        "@mui/x-date-pickers": "7.8.0",
        "@react-spring/web": "^9.7.4",
        "@types/telegram-web-app": "7.2.1",
        "@vkruglikov/react-telegram-web-app": "2.1.9",
        "axios": "1.7.2",
        "dayjs": "1.11.11",
        "effector": "23.2.2",
        "effector-react": "23.2.1",
        "moment": "2.30.1",
        "react": "18.3.1",
        "react-cookie": "7.1.4",
        "react-dom": "18.3.1",
